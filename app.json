{
  "expo": {
    "name": "youth-connect",
    "slug": "youth-connect",
    "version": "1.3.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#aaaa"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.youthconnect.youth-connect",
      "infoPlist": { 
        "NSCameraUsageDescription": "We needs camera permissions to allow you to upload profile photos.", 
        "NSPhotoLibraryAddUsageDescription": "We needs photo library permissions to allow you to upload profile images.",
         "NSPhotoLibraryUsageDescription": "We needs photo library permissions to allow you to upload profile images."
         },
      "assetBundlePatterns": ["**/*"]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#aaaa"
      },
      "package": "com.youthconnect.youth-connect"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "545ff028-5243-4b9f-a705-834bbe0097b9"
      }
    },
    "owner": "youthconnect"
  }
}
